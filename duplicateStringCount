using System;
					
public class Program
{
	public static void Main()
	{
		string name = "shubhamPanchals";
		string result = string.Empty;
		while (name .Length > 0)
		{
		int count = 0;
		for (int i = 0; i < name.Length; i++)
		{
			if (name[0] == name[i])
			{
			count++;
			}
		}
		result += name[0] + " = " + count + "\n";
		}
		Console.WriteLine(result);
	}
}
